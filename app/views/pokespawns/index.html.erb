<p id="notice"><%= notice %></p>

<h1>Listing Pokespawns</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pokespawns.each do |pokespawn| %>
      <tr>
        <td><%= pokespawn.name %></td>
        <td><%= pokespawn.latitude %></td>
        <td><%= pokespawn.longitude %></td>
        <td><%= pokespawn.user %></td>
        <td><%= link_to 'Show', pokespawn %></td>
        <td><%= link_to 'Edit', edit_pokespawn_path(pokespawn) %></td>
        <td><%= link_to 'Destroy', pokespawn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pokespawn', new_pokespawn_path %>
